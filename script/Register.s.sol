        ssvOperators[0].id = 56;
        ssvOperators[0].snapshot = getSnapshot(56);
        ssvOperators[0].fee = getOperatorFee(56);

        ssvOperators[1].owner = 0x96B5f8Ab8041182f23aA4E6ddc7ce0cbF608BBe0;
        ssvOperators[1].id = 77;
